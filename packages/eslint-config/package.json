{
  "name": "@yafh/eslint-config",
  "version": "0.6.1",
  "type": "commonjs",
  "description": "Eslint configuration for YanAndFish",
  "keywords": [
    "eslint",
    "config"
  ],
  "author": "YanAndFish <383904425@qq.com>",
  "homepage": "https://github.com/YanAndFish/eslint-config",
  "repository": {
    "type": "git",
